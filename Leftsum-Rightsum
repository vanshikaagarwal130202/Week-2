#include<bits/stdc++.h>
using namespace std;
int main()
{

    cout<<"Enter limit of array=";
    int n;cin>>n;
    int a[n];
    cout<<"Enter array elements=";
    for(int i=0;i<n;i++)
        cin>>a[i];
    int sum=0;
    for(int i=0;i<n;i++)
        sum+=a[i];
    int leftsum=a[0],rightsum=sum-(a[0]+a[1]);
    int i;
    for(i=1;i<n-1;i++)
    {
    if(leftsum==rightsum)
    {
    cout<<"Element is="<<a[i];
    break;
    }
    else
    {
        leftsum=leftsum+a[i];
        rightsum=rightsum-a[i+1];
    }
    }
    if(i==n-1)
        cout<<"Not found"<<endl;
}
